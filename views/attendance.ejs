                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!--attendance history-->
        <div class="modal fade" id="modalAttendanceHistory" tabindex="-1" role="dialog"
            aria-labelledby="modalAttendanceHistory" aria-hidden="true">
            <div class="modal-dialog modal-sm modal-dialog-centered " role="document">
                <div class="modal-content">
                    <div class="modal-body p-0">
                        <div class="card card-plain">
                            <div class="card-header pb-0 text-left">
                                <h4 class="font-weight-bolder text-dark text-gradient">{{studentCode}} {{studentName}}
                                </h4>
                                <small>{{unitCode}} {{unitName}}</small>
                            </div>
                            <div class="card-body pb-3">
                                <div class="row">
                                    <div class="col-4 text-start my-auto">
                                        <span class="badge badge-md badge-dot d-block">
                                            <i class="bg-info"></i>
                                            <span class="text-dark text-xs">Present</span>
                                        </span>
                                        <span class="badge badge-md badge-dot d-block">
                                            <i class="bg-dark"></i>
                                            <span class="text-dark text-xs">Absent</span>
                                        </span>
                                    </div>
                                    <div class="col-8 my-auto">
                                        <div class="chart">
                                            <canvas id="chart-pie" class="chart-canvas"></canvas>
                                        </div>
                                    </div>
                                </div>
